program conditionsTest;

var x, y: int;

main {
    x = 3;
    y = 0;

    if (x > 0) {
        print("x es positivo");
    } else {
        print("x no es positivo");
    };

    while (x > 0) do {
        print(x);
        x = x - 1;
    };

    if (y > 0) {
        print("y es positivo");
    };

    while (x != 0) do {
        print("No debe imprimirse");
    };
}

end